// === FUNÇÃO PRINCIPAL ===
async function sendScript(text, ms) {
  const lines = text.trim().split("\n").map(l => l.trim()).filter(l => l.length > 0);

  const textarea = document.querySelector('[contenteditable="true"][data-tab="10"]')
    || document.querySelector('[contenteditable="true"]');

  if (!textarea) {
    console.error("❌ Campo de mensagem não encontrado! Abra o chat no WhatsApp Web e tente de novo.");
    return;
  }

  for (let line of lines) {
    textarea.focus();

    // Simula digitação
    document.execCommand('insertText', false, line);
    
    // Dispara evento para o WhatsApp detectar
    textarea.dispatchEvent(new InputEvent('input', { bubbles: true }));

    // Pressiona Enter para enviar
    const enterEvent = new KeyboardEvent('keydown', {
      bubbles: true,
      cancelable: true,
      key: 'Enter',
      code: 'Enter'
    });
    textarea.dispatchEvent(enterEvent);

    await new Promise(r => setTimeout(r, ms));
  }
  console.log("✅ Script enviado com sucesso!");
}

// Executa
sendScript(roteiro, delay);

// Executa
sendScript(roteiro, delay);
